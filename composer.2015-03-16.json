{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.4",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/dbal": "<2.5",
        "doctrine/data-fixtures": "1.0.*@dev",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-fixtures-bundle": "2.2.*@dev",
        "gedmo/doctrine-extensions": "dev-master",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "2.5.*@dev",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "stof/doctrine-extensions-bundle": "dev-master",
        "friendsofsymfony/user-bundle": "1.3.x-dev as dev-master",
        "friendsofsymfony/jsrouting-bundle": "@stable",
        "friendsofsymfony/oauth-server-bundle": "@stable",
        "jms/serializer-bundle": "*",
        "sonata-project/block-bundle": "2.2.7",
        "sonata-project/formatter-bundle": "~2.3",
        "sonata-project/ecommerce": "dev-master",
        "sonata-project/comment-bundle": "dev-master",
        "sonata-project/news-bundle": "dev-master",
        "sonata-project/datagrid-bundle": "dev-master",
        "sonata-project/cache-bundle": "~2.0",
        "sonata-project/user-bundle": "dev-master",
        "knplabs/doctrine-behaviors": "dev-master",
        "knplabs/knp-paginator-bundle": "2.4.*",
        "lunetics/locale-bundle": "2.2.*",
        "excelwebzone/recaptcha-bundle": "dev-master",
        "creof/doctrine2-spatial": "dev-master",
        "map2u/core-bundle": "dev-master",
        "map2u/leaflet-bundle": "dev-master",
        "map2u/forum-bundle": "dev-master",
        "map2u/dashboard-bundle": "dev-master",
        "map2u/web-bundle": "dev-master",
        "map2u/mapfish-print-bundle": "dev-master",
        "a2lix/i18n-doctrine-bundle": "dev-master",
        "a2lix/translation-form-bundle": "2.*@dev",
        "suncat/mobile-detect-bundle": "0.10.*",
        "paradigmate/image-bundle": "dev-master",
        "stfalcon/tinymce-bundle": "0.3.*@dev",
        "ibrows/newsletter-bundle": "*",
        "hwi/oauth-bundle": "0.4.*@dev"



    },
    "require-dev": {
        "phpunit/php-code-coverage": "~2.0",
        "phpunit/phpunit-mock-objects": "2.4.*@dev",
        "nikic/php-parser": "dev-master",
        "phpunit/phpunit": "dev-master",
        "theseer/fxsl": "dev-master",
        "theseer/phpdox": "dev-master",
        "squizlabs/php_codesniffer": "dev-master",
        "phpmd/phpmd": "dev-master",
        "phploc/phploc": "dev-master",
        "sebastian/phpcpd": "dev-master",
        "sebastian/global-state": "1.0.*@dev",
        "sebastian/comparator": "1.1.*@dev",
        "phpdocumentor/phpdocumentor": "dev-master",
        "mayflower/php-codebrowser": "dev-master",
        "pear/pear_exception": "dev-master",
        "phing/phing": "dev-master",
        "behat/behat": ">=2.4.4",
        "behat/mink-extension": "dev-master",
        "behat/mink-zombie-driver": "dev-master",
        "behat/mink-browserkit-driver": "dev-master",
        "behat/mink": "dev-master",
        "behat/symfony2-extension": "dev-master",
        "phpspec/phpspec": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uCoreBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uLeafletBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uMapfishPrintBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uForumBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uDashboardBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uWebBundle.git"
        }],
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
