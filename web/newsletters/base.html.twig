        #}

            <link rel="stylesheet" href="/bundles.old/sonatajquery/themes/flick/jquery-ui-1.8.16.custom.css" type="text/css" media="all" />
            <link rel="stylesheet" href="/bundles.old/sonataadmin/x-editable/css/bootstrap-editable.css" type="text/css" media="all" />
            <link rel="stylesheet" href="/bundles.old/sonataadmin/bootstrap/css/bootstrap.min.css" type="text/css" media="all"  />
            <link rel="stylesheet" href="/bundles.old/sonataadmin/bootstrap/css/bootstrap-responsive.min.css" type="text/css" media="all" />

            <link rel="stylesheet" href="/bundles.old/sonataadmin/select2/select2.css" type="text/css" media="all" />
            <!-- base application asset -->
            <link rel="stylesheet" href="/bundles.old/sonataadmin/css/layout.css" type="text/css" media="all" />
            <link rel="stylesheet" href="/bundles.old/sonataadmin/css/colors.css" type="text/css" media="all" />

            <!-- ckeditor -->
            <link rel="stylesheet" href="/bundles.old/sonatamarkitup/markitup/markitup/skins/sonata/style.css" type="text/css" media="all" />
            <link rel="stylesheet" href="/bundles.old/sonatamarkitup/markitup/markitup/sets/markdown/style.css" type="text/css" media="all" />
            <link rel="stylesheet" href="/bundles.old/sonatamarkitup/markitup/markitup/sets/html/style.css" type="text/css" media="all" />
            <link rel="stylesheet" href="/bundles.old/sonatamarkitup/markitup/markitup/sets/textile/style.css" type="text/css" media="all" />
            <link rel="stylesheet" href="/bundles.old/map2ucore/css/loginform.css" type="text/css" media="all" />
            <link rel="stylesheet" href="/bundles.old/map2ucore/css/bootstrapicon.css" type="text/css" media="all" />
            <style>
                .container-fluid.span9 {
                    margin: 0 auto;
                    
                    width: 960px;
                }
                body.sonata-bc {
                    margin: 0;
                    padding-top: 45px;
                }
                .sonata-bc .row , .sonata-bc .page_header {
                    margin: 0px;
                    padding: 0px;
                }
                .sonata-bc .page_header_content {
                    
                        border: 1px solid #008000;
                        height: 70px;
                        margin: 3px 10px;
                   
                }
                .sonata-bc .leaflet_content {
                    bottom: 0;
                    overflow: hidden;
                    position: absolute;
                   
                    width: 100%;

                }
            </style>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body class="sonata-bc">
        {% block body %}
            {% block page_header %}
                <div class="row page-header">
                    {% block fixed_top_menu %}
                        {{ sonata_block_render({ 'type': 'map2ucorebundle.block.fixed_topmenu' }, { 'mode': 'public'}) }}
                    {% endblock %}                     
                    {% block page_header_content %}

                        {{ sonata_block_render({ 'type': 'map2ucorebundle.block.page_header_content' }, { 'mode': 'public'}) }}

                    {% endblock %}         
                </div>
            {% endblock %}
            {% block maincontent %}
            {% endblock %}
        {% endblock %}
        {% block page_javascripts %}
            <script type="text/javascript">

                window.SONATA_CONFIG = {
                    CONFIRM_EXIT: {% if admin_pool is defined and admin_pool.getOption('confirm_exit') %}true{% else %}false{% endif %},
                    USE_SELECT2: {% if admin_pool is defined and admin_pool.getOption('use_select2') %}true{% else %}false{% endif %}
                };
                window.SONATA_TRANSLATIONS = {
                    CONFIRM_EXIT: '{{ 'confirm_exit'|trans({}, 'SonataAdminBundle')|escape('js') }}'
                };

            </script> 
            <script>window.jQuery || document.write('<script src="{{ asset('/bundles/map2ucore/js/jquery-1.8.3.min.js')}}"><\/script>');</script>
            <script src="/bundles.old/sonatajquery/jquery-ui-1.8.23.js" type="text/javascript"></script>
            <script src="/bundles.old/sonatajquery/jquery-ui-i18n.js" type="text/javascript"></script>

            <script src="{{ asset('bundles/map2ucore/js/jquery.validationEngine-en.js') }}"></script>
            <script src="{{ asset('bundles/map2ucore/js/jquery.validationEngine.js') }}"></script>

            {{ render(controller("Map2uCoreBundle:Welcome:loginformjs")) }}

            <script src="/bundles.old/sonataadmin/jquery/jquery.form.js" type="text/javascript"></script>
            <script src="/bundles.old/sonataadmin/jquery/jquery.confirmExit.js" type="text/javascript"></script>    
            <script src="/bundles.old/sonataadmin/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
            <script src="/bundles.old/sonataadmin/x-editable/js/bootstrap-editable.min.js"></script>
            <script src="/bundles.old/sonataadmin/select2/select2.min.js" type="text/javascript"></script>
            <script src="/bundles.old/sonataadmin/base.js" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>
