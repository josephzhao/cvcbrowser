{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.4",
        "doctrine/orm": "2.4.*",
        "doctrine/doctrine-bundle": "1.2.*",
        "doctrine/doctrine-fixtures-bundle": "2.1.*@dev",
        "doctrine/data-fixtures": "1.0.*@dev",
        "twig/twig": "1.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "whiteoctober/tcpdf-bundle": "dev-master",
        "gedmo/doctrine-extensions": "dev-master",
        "tecnick.com/tcpdf": "*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "lunetics/locale-bundle": "dev-master",
        "symfony/monolog-bundle": "~2.4",
        "friendsofsymfony/jsrouting-bundle": "dev-master",
        "friendsofsymfony/user-bundle": "1.3.x-dev as dev-master",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "creof/doctrine2-spatial": "dev-master",
        "stof/doctrine-extensions-bundle": "~1.1@dev",
        "map2u/core-bundle": "dev-master",
        "map2u/leaflet-bundle": "dev-master",
        "map2u/web-bundle": "dev-master",
        "map2u/forum-bundle": "dev-master",
        "map2u/dashboard-bundle": "dev-master",
        "map2u/mapfish-print-bundle": "dev-master",
         "paradigmate/image-bundle": "dev-master",
        "hwi/oauth-bundle": "0.4.*@dev",
        "excelwebzone/recaptcha-bundle": "dev-master",
        "a2lix/i18n-doctrine-bundle": "dev-master",
        "a2lix/translation-form-bundle": "2.*@dev",
        "jms/serializer-bundle": "*",
        "knplabs/knp-menu": "*",
        "knplabs/knp-menu-bundle": "*",
        "knplabs/doctrine-behaviors": "dev-master",
        "knplabs/knp-paginator-bundle": "2.4.*",
        "sonata-project/core-bundle": "~2.2@dev",
        "sonata-project/admin-bundle": "~2.2@dev",
        "sonata-project/media-bundle": "~2.2@dev",
        "sonata-project/easy-extends-bundle": "~2.1@dev",
        "sonata-project/formatter-bundle": "~2.2@dev",
        "sonata-project/intl-bundle": "~2.2@dev",
        "sonata-project/datagrid-bundle": "~2.2@dev",
        "sonata-project/user-bundle": "~2.2@dev",
        "sonata-project/page-bundle": "~2.2@dev",
        "sonata-project/doctrine-orm-admin-bundle": "~2.2@dev",
        "sonata-project/news-bundle": "dev-master",
        "sonata-project/classification-bundle": "~2.2@dev",
        "sonata-project/cache-bundle": "dev-master",
        "sonata-project/notification-bundle": "dev-master",
        "sonata-project/seo-bundle": "dev-master",
       
        "sonata-project/markitup-bundle": "dev-master",
        "ibrows/newsletter-bundle": "*",
        "incenteev/composer-parameter-handler": "~2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uCoreBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uLeafletBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uForumBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uDashboardBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uMapfishPrintBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uWebBundle.git"
        }],
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
