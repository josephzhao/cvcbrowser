input[type="button"]:last-child,input[type="submit"]:last-child,input[type="reset"]:last-child,a.button:last-child{margin-bottom:0}
.buttons{min-width:200px}.buttons input[type="submit"],.buttons input[type="button"],.buttons input[type="reset"],.buttons .button{box-sizing:border-box;float:left;border-radius:0;margin:0;min-width:75px;max-width:180px;border-right:1px solid white;text-overflow:ellipsis;white-space:nowrap;overflow:hidden}
.buttons input:first-child,.buttons .button:first-child{border-radius:2px 0 0 2px}.buttons input:last-child,.buttons .button:last-child{border-radius:0 2px 2px 0;border-right-width:0}.buttons input:only-child,.buttons .button:only-child,.buttons *[value="Hide"]+input:last-child,.buttons *[value="Hide"]+.button:last-child{border-radius:2px;border-right-width:0}.buttons *[value="Hide"]+input:nth-child(3),.buttons *[value="Hide"]+.button:nth-child(3){border-radius:0 2px 2px 0;border-right-width:0}
.buttons *[value="Hide"]+input:nth-child(2):not(:last-child),.buttons *[value="Hide"]+.button:nth-child(2):not(:last-child){border-radius:2px 0 0 2px;border-right-width:1px}


