{#% extends '::base.html.twig' %#}

{% block body -%}
<h1>Logo edit</h1>

<form action="{{path("logo_update", {'id': entity.id})}}" method="post" {{ form_enctype(edit_form) }}>

    {% for flashMessage in app.session.flashbag.get('notice') %}
    <div class="flash-notice">
            {{ flashMessage }}
    </div>
    {% endfor %}



    <table>


        <tr>
            <td>{{ form_label(edit_form.imagename) }}</td>
            <td>{{ form_widget(edit_form.imagename) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(edit_form.imagefile) }}</td>
            <td>{{ form_widget(edit_form.imagefile) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(edit_form.imageFilename) }}</td>
            <td>{{ form_widget(edit_form.imageFilename) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(edit_form.imageTip) }}</td>
            <td>{{ form_widget(edit_form.imageTip) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(edit_form.websiteUrl) }}</td>
            <td>{{ form_widget(edit_form.websiteUrl) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(edit_form.showSeq) }}</td>
            <td>{{ form_widget(edit_form.showSeq) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(edit_form.enabled) }}</td>
            <td>{{ form_widget(edit_form.enabled) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(edit_form.description) }}</td>
            <td>{{ form_widget(edit_form.description) }}</td>
        </tr>
    </table>
   {{ form_rest(edit_form) }}
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('logo') }}">
            Back to the list
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
