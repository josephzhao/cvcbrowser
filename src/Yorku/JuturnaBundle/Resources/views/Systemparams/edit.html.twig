

{% block body -%}
    <h1>System Parameter Edit</h1>

    <form action="{{ path('systemparams_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
        <input type="hidden" name="_method" value="PUT" />
        {#{ form_widget(edit_form) }#}
 
        <table class="records_list">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Value</th>
                </tr>
            </thead>            


            <tbody>

                <tr>
                    <td>{{ form_label(edit_form.logo1Name) }}</td>
                    <td>{{ form_widget(edit_form.logo1Name,{required:true}) }}</td>
                </tr>
                <tr>
                    <td>{{ form_label(edit_form.logo1Url) }}</td>
                    <td>{{ form_widget(edit_form.logo1Url) }}</td>
                </tr>
                <tr>
                    <td>{{ form_label(edit_form.logo1Filename) }}</td>
                    <td>{{ form_widget(edit_form.logo1Filename, {'attr': {'disabled': 'disabled'}}) }}</td>
                </tr>
                <tr>
                    <td>{{ form_label(edit_form.file1) }}</td>
                    <td>{{ form_widget(edit_form.file1) }}</td>
                </tr>
                <tr>
                    <td colspan="2">
                        <hr/>
                    </td>
                </tr>
                <tr>

                    <td>{{ form_label(edit_form.logo2Name) }}</td>
                    <td>{{ form_widget(edit_form.logo2Name) }}</td>
                </tr>
                <tr>
                    <td>{{ form_label(edit_form.logo2Url) }}</td>
                    <td>{{ form_widget(edit_form.logo2Url) }}</td>
                </tr>
                <tr>
                    <td>{{ form_label(edit_form.logo2Filename) }}</td>
                    <td>{{ form_widget(edit_form.logo2Filename,{'attr': {'disabled': 'disabled'}}) }}</td>
                </tr>
                <tr>
                    <td>{{ form_label(edit_form.file2) }}</td>
                    <td>{{ form_widget(edit_form.file2) }}</td>
                </tr>
                <tr>
                    <td colspan="2">
                        <hr/>
                    </td>
                </tr>

                <tr>
                    <td>{{ form_label(edit_form.logo3Name) }}</td>
                    <td>{{ form_widget(edit_form.logo3Name) }}</td>
                </tr>
                <tr>
                    <td>{{ form_label(edit_form.logo3Url) }}</td>
                    <td>{{ form_widget(edit_form.logo3Url) }}</td>
                </tr>
                <tr>
                    <td>{{ form_label(edit_form.logo3Filename) }}</td>
                    <td>{{ form_widget(edit_form.logo3Filename, {'attr': {'disabled': 'disabled'}}) }}</td>
                </tr>
                <tr>
                    <td>{{ form_label(edit_form.file3) }}</td>
                    <td>{{ form_widget(edit_form.file3) }}</td>
