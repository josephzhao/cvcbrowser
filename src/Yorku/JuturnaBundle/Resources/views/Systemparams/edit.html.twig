                <tr>
                    <td colspan="2">
                        <hr/>
                    </td>
                </tr>

                <tr>
                    <td>{{ form_label(edit_form.masteremail) }}</td>
                    <td>{{ form_widget(edit_form.masteremail) }}</td>
                </tr>

                <tr>
                    <td>{{ form_label(edit_form.geoserverHost) }}</td>
                    <td>{{ form_widget(edit_form.geoserverHost) }}</td>
                </tr>
                <tr>
                    <td>{{ form_label(edit_form.geoserverPort) }}</td>
                    <td>{{ form_widget(edit_form.geoserverPort) }}</td>
                </tr>
                <tr>
                    <td>{{ form_label(edit_form.geoserverWorkspace) }}</td>
                    <td>{{ form_widget(edit_form.geoserverWorkspace) }}</td>
                </tr>
 {{ form_rest(edit_form) }}
            </tbody>
        </table>
    
           <p>
            <button type="submit">Update</button>
        </p>
    </form>

        <ul class="record_actions">
    <li>
        <a href="{{ path('systemparams') }}">
            Back to the list
        </a>
    </li>
    <li>
        <form action="{{ path('systemparams_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
