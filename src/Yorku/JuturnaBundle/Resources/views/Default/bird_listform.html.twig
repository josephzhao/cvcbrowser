{# empty Twig template #}
<div id="homepage_mapbird_id" class="container-fluid">
    <div class="row-fluid">
       {% if species is defined and species|length > 0 %}
    
        {% set i=0 %}
            {% for specie in species %}
         <div style="border-bottom: 1px #E0E1E2 dotted">
              <a href="{{path('species_show')}}?id={{specie.getId()}}" target="_blank">{{specie.getSpeciesName()}}</a>
                {% set images=specie.getImages()|unserialize %}
                {% if images|length > 0 %}
                                    
                                   <a href="{{path('species_show')}}?id={{specie.getId()}}" target="_blank"><img style="border: 1px #0000FF solid" src="{{ asset('uploads/images/species/pictures/')}}{{images[0]}}" width="20px" height="16px" style="width:20px;height:16px;"/></a>
                {% else %}
                                   
                {% endif %}
                          
                {% set i = i+1 %}
         </div>                            
            {% endfor %} 
                        
        {% endif %}
        
    </div>
</div>