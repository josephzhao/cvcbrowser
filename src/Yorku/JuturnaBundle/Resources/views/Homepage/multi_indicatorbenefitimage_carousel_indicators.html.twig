{% set images=indicatorbenefitimage.getFileName()|unserialize %}
{% if images|length > 0 %}
    {% for image in images %}
        <li data-target="#indicator_benefit_flash" data-slide-to="{{i}}" {% if i==0 %} class="active" {% endif %}>
        </li>
        {% set i=i+1 %}
    {% endfor %}
{% endif %}
