
<div id="leftsidebar_indicator_benefit_flash" style="border-radius: 5px;">
    <div class="carousel-inner" style="border-radius: 5px;">
        {% for indicatorbenefitimage in entities.indicatorbenefitimages %}
            {% set images=indicatorbenefitimage.getFileName()|unserialize %}
            {% if images|length > 0 %}
                {% for image in images %}
                    {% autoescape 'html' %}
                        <img style="border-radius: 5px;width:300px;height:200px;overflow: hidden;" src="{{ asset('uploads/images/indicatorbenefitimages/'~indicatorbenefitimage.id~"/")}}{{image}}" width="100%" alt="{{indicatorbenefitimage.getAltText()}}">
                    {% endautoescape %}
                    <div class="carousel-caption">
                        <h5>{{indicatorbenefitimage.getImageTitle()}}</h5>
                        <p>{{indicatorbenefitimage.getImageCaption()}}</p>
                    </div>
                {% endfor %}
            {% endif %}
        {% endfor %}
    </div>
</div>
